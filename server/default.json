{
  "datacenter": "%%DATACENTER%%",
  "data_dir": "/tmp/data",
  "server": true,
  "enable_script_checks": true,
  "ui": true,
  "bootstrap_expect": %%BOOTSTRAP_EXPECT%%,
  "primary_datacenter": "%%DATACENTER%%",
  "acl": {
    "enabled": true,
    "default_policy": "deny",
    "enable_token_persistence": true,
    "tokens": {
      "initial_management": "%%CONSUL_HTTP_TOKEN%%"
    }
  },
  "node_name": "%%SERVER_NODE_NAME%%",
  "log_level": "INFO",
  "domain": "consul",
  "client_addr": "0.0.0.0",
  "advertise_addr": "%%SERVER_ADVERTISE_ADDR%%"
}
